<?xml version="1.0" encoding="utf-8"?>
<UrlConfig
    debug="false"
    debugBaseHttp="http://121.40.63.64:80/jk_api/"
    debugBaseHttps=""
    defaultVersion="v1"
    releaseBaseHttp="http://192.168.10.55/jk_api/"
    releaseBaseHttps=""
    signName="key"
    tokenName="token">
    <!-- 获取客户端token -->
    <Node
        class="java.lang.String"
        id="@+id/api_user_token"
        ids="api_user_token"
        method="get"
        needToken="false"
        url="user/token" />
    <Node
        class="java.lang.String"
        id="@+id/api_user_login"
        ids="api_user_login"
        encrypt="loginInfo"
        needToken="true"
        url="user/login" />
    <Node
        class=".okhttp.entity.DefaultApiEntity"
        id="@+id/api_user_logout"
        ids="api_user_logout"
        needToken="true"
        url="user/logout" />
    <Node
        class=".okhttp.entity.DefaultApiEntity"
        id="@+id/api_set_push_channel"
        ids="api_set_push_channel"
        method="get"
        needToken="true"
        url="user/set/channel" />
    <Node
        class="[.entity.OrderListDataEntity"
        id="@+id/api_order_list"
        ids="api_order_list"
        method="get"
        needToken="true"
        url="order/list" />
    <Node
        class="[.entity.OrderGetrecordDataEntity"
        id="@+id/api_order_getRecord"
        ids="api_order_getRecord"
        method="get"
        needToken="true"
        url="order/getRecord" />
    <Node
        class=".entity.DefaultApiEntity"
        id="@+id/api_order_update"
        ids="api_order_update"
        method="get"
        needToken="true"
        url="order/update" />
    <Node
        class=".entity.DefaultApiEntity"
        id="@+id/api_order_ajaxSaveFile"
        ids="api_order_ajaxSaveFile"
        method="post"
        needToken="false"
        url="order/ajaxSaveFile" />
</UrlConfig>